{
  "name": "chaoticbacktalk",
  "version": "3.5.0",
  "description": "Discord Bot for the Chaotic Discord",
  "author": "Danude Sandstorm",
  "scripts": {
    "start": "NODE_ENV=development babel-watch src/bot.js --extensions \".js,.ts\"",
    "prebuild": "if [ -f err.log ]; then mv err.log err.old.log; fi",
    "build": "babel src -d build --copy-files --extensions \".js,.ts\"",
    "forever": "npm run build && forever -o out.log -e err.log start build/bot.js",
    "rebuild": "npm run stop; npm run forever",
    "stop": "forever stop build/bot.js",
    "clean": "rm -rf build/",
    "gitauto": "forever start node_modules/git-auto-pull/git-auto-pull.js ../../ \"npm run rebuild\"",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaoticbackup/discordbot.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.6.0",
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "fs-extra": "^7.0.1",
    "jquery": "^3.4.1",
    "jsdom": "^13.2.0",
    "lokijs": "^1.5.7",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.1.266",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.6.0",
    "@types/lokijs": "^1.5.2",
    "@types/node": "^12.7.5",
    "babel-eslint": "^10.0.3",
    "babel-watch": "^7.0.0",
    "eslint": "^6.4.0",
    "forever": "^1.0.0",
    "git-auto-pull": "^1.0.9",
    "typescript": "^3.6.3",
    "webpack": "^3.12.0"
  }
}
