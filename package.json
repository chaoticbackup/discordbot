{
  "name": "chaoticbacktalk",
  "version": "3.5.0",
  "description": "Discord Bot for the Chaotic Discord",
  "author": "Danude Sandstorm",
  "scripts": {
    "start": "NODE_ENV=development node start.js",
    "start:scan": "APP_ENV=scan NODE_ENV=development node start.js",
    "start:forum": "APP_ENV=forum NODE_ENV=development node start.js",
    "build": "babel src -d build --delete-dir-on-start --copy-files --extensions \".js,.ts\"",
    "pre_forever": "yarn install; if [ -f out.log ]; then cat out.log >> out.old.log; fi;",
    "forever": "yarn pre_forever; yarn build && forever -o out.log -e out.log start build/bot.js",
    "rebuild": "yarn stop; yarn forever",
    "stop": "forever stop build/bot.js",
    "gitauto": "forever start node_modules/git-auto-pull/git-auto-pull.js ../../ \"yarn rebuild\"",
    "lint": "tsc --noEmit; eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "migrate": "node ./migrations/_run.js"
  },
  "repository": "git+https://github.com/chaoticbackup/discordbot.git",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "bufferutil": "^4.0.3",
    "chokidar": "^3.5.2",
    "core-js": "^3.18.0",
    "discord-paginationembed": "^1.0.3",
    "discord.js": "^11.6.4",
    "express": "^4.17.1",
    "forever": "^3.0.4",
    "fs-extra": "^10.0.0",
    "git-auto-pull": "^1.0.9",
    "jquery": "^3.6.0",
    "jsdom": "^14.1.0",
    "lokijs": "^1.5.12",
    "moment": "^2.29.1",
    "mongodb": "^4.4.0",
    "node-fetch": "^2.6.5",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^3.9.10",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-json-strings": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.6",
    "@types/jsdom": "^12.2.4",
    "@types/lokijs": "^1.5.7",
    "@types/node": "^14.17.18",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0"
  }
}
